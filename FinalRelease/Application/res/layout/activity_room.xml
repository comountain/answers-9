<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.RoomActivity">

    <Chronometer
        android:id="@+id/_chro_room"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        android:layout_weight="1"

        android:gravity="center" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayout9"
            android:layout_width="282dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="150px"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.922"
            tools:layout_editor_absoluteX="-16dp">

            <TextView
                android:id="@+id/room_name1"
                android:layout_width="69dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/room_name2"
                android:layout_width="69dp"
                android:layout_height="20dp"
                android:layout_marginLeft="144dp"
                android:gravity="center"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout0"
            android:layout_width="282dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0px"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.922"
            tools:layout_editor_absoluteX="-16dp">

            <ImageView
                android:id="@+id/room_player1"
                android:layout_width="69dp"
                android:layout_height="54dp"
                android:layout_marginLeft="0dp"
                android:src="@drawable/back_main" />



            <ImageView
                android:id="@+id/room_player2"
                android:layout_width="69dp"
                android:layout_height="54dp"
                android:layout_marginLeft="144dp"
                android:src="@drawable/back_main" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout10"
            android:layout_width="282dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="100px"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.922"
            tools:layout_editor_absoluteX="-16dp">

            <TextView
                android:id="@+id/room_name3"
                android:layout_width="69dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/room_name4"
                android:layout_width="69dp"
                android:layout_height="20dp"
                android:layout_marginLeft="144dp"
                android:gravity="center"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout01"
            android:layout_width="282dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0px"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.922"
            tools:layout_editor_absoluteX="-16dp">

            <ImageView
                android:id="@+id/room_player3"
                android:layout_width="69dp"
                android:layout_height="54dp"
                android:layout_marginLeft="0dp"
                android:src="@drawable/back_main" />

            <ImageView
                android:id="@+id/room_player4"
                android:layout_width="69dp"
                android:layout_height="54dp"
                android:layout_marginLeft="144dp"
                android:src="@drawable/back_main" />
        </LinearLayout>

    </LinearLayout>


    <Button
        android:id="@+id/room_ret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="6dp"
        android:text="返回"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_start_fvf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="272dp"
        android:text="开始对决"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/list_invitefriend"
        android:layout_width="409dp"
        android:layout_height="409dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="320dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout5" />


</androidx.constraintlayout.widget.ConstraintLayout>
